class Print:
	def __init__(self,name,gender='male',age):
		self.name=name
        self.gender=gender
        self.age=age
		
	def kc(self):
		data='%s,性别:%s,今年%s岁,喜欢上山砍柴'%(self.name,self.gender,self.age)
        print(data)
    
    
